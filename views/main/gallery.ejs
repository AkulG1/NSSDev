<% layout('layout') -%>
<!--Work-->
<div class="section scrollspy" id="work">
    <div class="container">

        <div class="row">
            <div  class="col s12">
                <h3 class="center header text_h2"><span class="span_h2"> Gallery</span></h3>
            </div>

            <% for(var i = 0; i< gallery.length;i++) {%>
            <div class="container">
            <div class="col s12 m6 l4 center" style="margin-bottom:15px;">
                  <img class="materialboxed" width="400"  height="250" data-caption="<%= gallery[i].name %>" src="/img/gallery/<%= gallery[i].image %>">
            </div>
          </div>
            <% } %>

        </div>
    </div>
</div>
